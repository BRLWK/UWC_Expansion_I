      </li>
    </subSounds>
  </SoundDef>

  <SoundDef>
    <defName>CompCom_Working</defName>  
    <sustain>True</sustain>  
    <context>MapOnly</context>
    <priorityMode>PrioritizeNearest</priorityMode>
    <subSounds>
      <li>
        <muteWhenPaused>True</muteWhenPaused>      
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>SustainerCompAGL</clipFolderPath>
          </li>
        </grains>      
        <volumeRange>15</volumeRange>
        <pitchRange>0.90~0.91</pitchRange>
      </li>
    </subSounds>
  </SoundDef>
  <!-- grenade is thinking -->
  <SoundDef>
    <defName>CompCom_Reload_Loading_Ready</defName>  
    <sustain>True</sustain>  
    <context>MapOnly</context>
    <priorityMode>PrioritizeNearest</priorityMode>
    <sustainStartSound>Reload_Single_AGL3</sustainStartSound>  
    <sustainStopSound>Ready_Fire_AGL3</sustainStopSound>  
    <subSounds>
      <li>
        <muteWhenPaused>True</muteWhenPaused>      
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>SustainerCompAGL</clipFolderPath>
          </li>
        </grains>      
        <volumeRange>18~18</volumeRange>      
        <pitchRange>0.90~0.91</pitchRange>
      </li>
    </subSounds>
  </SoundDef>

  <SoundDef>
