  <SoundDef>
    <defName>Reload_Single_AGL3</defName>  
    <context>MapOnly</context> 
    <eventNames />  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>ReloadAGL</clipFolderPath>
          </li>
        </grains>      
