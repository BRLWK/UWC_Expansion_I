<Defs>

<!-- =======================Fire======================= -->

<!-- Audio credit to johanwesting @ Freesound.org; "377786__johanwestling__gun_22mm_75mm_9mm_close_single_shots_m10.wav" -->

   <SoundDef>
    <defName>Fire_K10_Pistol</defName>  
    <context>MapOnly</context> 
    <eventNames />  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>k10</clipFolderPath>
          </li>
        </grains>      
        <volumeRange>
          <min>8.8</min>        
          <max>10.2</max>
        </volumeRange>      
        <pitchRange>
          <min>0.9</min>        
          <max>1.1</max>
        </pitchRange>
      </li>
    </subSounds>
  </SoundDef>

