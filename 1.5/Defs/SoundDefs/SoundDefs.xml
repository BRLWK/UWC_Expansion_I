      </li>
    </subSounds>
  </SoundDef>

  <SoundDef>
    <defName>Ready_Fire_AGL3</defName>  
    <context>MapOnly</context> 
    <eventNames />  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>SmartAimShort</clipPath>
          </li>
        </grains>      
        <volumeRange>
          <min>18</min>        
          <max>18</max>
        </volumeRange>      
