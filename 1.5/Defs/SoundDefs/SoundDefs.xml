  <!-- HVT-->

    <SoundDef>
      <defName>Shot_CBR_Heavy</defName>  
      <eventNames/>  
      <context>MapOnly</context>
      <maxSimultaneous>1</maxSimultaneous>  
      <subSounds>
        <li>
          <grains>
            <li Class="AudioGrain_Clip">
              <clipPath>Caseless/Shot_cbatrifle</clipPath>
            </li>
          </grains>      
          <volumeRange>
            <min>40</min>        
            <max>40</max>
          </volumeRange>      
          <pitchRange>
            <min>1.071765</min>        
            <max>1.075765</max>
          </pitchRange>
          <filters>
            <li Class="SoundFilterEcho">
              <delay>1112.654</delay>          
              <decayRatio>0.5433328</decayRatio>          
              <wetMix>0.1333435</wetMix>          
              <dryMix>1</dryMix>
            </li>
          </filters> 
        </li>
      </subSounds>
    </SoundDef>

</Defs>