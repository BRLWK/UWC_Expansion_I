  <ThingDef ParentName="BuildingBase">
    <defName>UWC_Entrenchment</defName>
    <label>Entrenchment</label>
    <thingClass>Building</thingClass>
    <description>Weak but effective cover - A simple mound of dirt reinforced with local materials. Used in-field when no better options are avaliable.</description>
    <graphicData>
      <texPath>Things/Building/Linked/Barricade_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <linkType>Basic</linkType>
      <linkFlags>
        <li>Barricades</li>
      </linkFlags>
      <color>(139,122,87)</color>
    </graphicData>
    <constructEffect>ConstructDirt</constructEffect>
    <repairEffect>ConstructDirt</repairEffect>
    <uiIconPath>Things/Building/Linked/Barricade_MenuIcon</uiIconPath>
    <uiIconScale>1.2</uiIconScale>
    <altitudeLayer>Building</altitudeLayer>
    <building>
      <ai_chillDestination>false</ai_chillDestination>
      <isInert>true</isInert>
      <isFence>true</isFence>
      <isPlaceOverableWall>true</isPlaceOverableWall>
      <blueprintGraphicData>
        <texPath>Things/Building/Linked/Barricade_Blueprint_Atlas</texPath>
      </blueprintGraphicData>
      <paintable>true</paintable>
    </building>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToBuild>160</WorkToBuild>
      <Flammability>0</Flammability>
      <Cleanliness>-10</Cleanliness>
      <Beauty>-3</Beauty>
    </statBases>
    <costList>
    </costList>
    <damageMultipliers>
      <li>
        <damageDef>Bomb</damageDef>
        <multiplier>4</multiplier>
      </li>
      <li>
        <damageDef>Flame</damageDef>
        <multiplier>0</multiplier>
      </li>
      <li>
        <damageDef>Thump</damageDef>
        <multiplier>4</multiplier>
      </li>
    </damageMultipliers>
    <terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
    <placingDraggableDimensions>1</placingDraggableDimensions>
    <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    <passability>PassThroughOnly</passability>
    <pathCost>42</pathCost>
    <pathCostIgnoreRepeat>true</pathCostIgnoreRepeat>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>0.6</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <coversFloor>false</coversFloor>
    <tickerType>Never</tickerType>
    <stealable>false</stealable>
    <rotatable>false</rotatable>
    <selectable>true</selectable>
    <neverMultiSelect>true</neverMultiSelect>
    <holdsRoof>false</holdsRoof>
    <staticSunShadowHeight>0.2</staticSunShadowHeight>
    <blockLight>false</blockLight>
    <comps>
	</comps>
  </ThingDef>

</Defs>